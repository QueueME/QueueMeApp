<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_start_session"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.queueme.StartSession">

    <Button
        android:text="Start Session"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/queue"
        tools:layout_conversion_absoluteX="125dp"
        tools:layout_conversion_absoluteY="231dp"
        tools:layout_conversion_absoluteWidth="128dp"
        tools:layout_conversion_absoluteHeight="48dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="200dp"
        app:layout_constraintHorizontal_bias="0.52" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/emnekode"
        tools:layout_conversion_absoluteX="163dp"
        tools:layout_conversion_absoluteY="53dp"
        tools:layout_conversion_absoluteWidth="58dp"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="159dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/emnenavn"
        tools:layout_conversion_absoluteX="174dp"
        tools:layout_conversion_absoluteY="102dp"
        tools:layout_conversion_absoluteWidth="58dp"
        tools:layout_conversion_absoluteHeight="16dp"
        app:layout_constraintBottom_toTopOf="@+id/queue"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/emnekode"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintVertical_bias="0.75" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/enter"
        android:text="YOU'R ABOUT TO START A QUEUE IN:"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toTopOf="@+id/emnekode"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
